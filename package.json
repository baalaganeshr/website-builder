{
  "name": "website-builder",
  "version": "1.0.0",
  "description": "A full-stack application for generating websites from screenshots and text prompts using local AI models",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --port 7001",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && pytest",
    "setup": "npm run setup:frontend && npm run setup:backend",
    "setup:frontend": "cd frontend && npm install",
    "setup:backend": "cd backend && pip install -r requirements.txt",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "start": "npm run dev",
    "lint": "npm run lint:frontend",
    "lint:frontend": "cd frontend && npm run lint"
  },
  "workspaces": [
    "frontend"
  ],
  "keywords": [
    "ai",
    "website-builder",
    "screenshot-to-code",
    "ollama",
    "react",
    "fastapi",
    "typescript",
    "python"
  ],
  "author": "baalaganeshr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/baalaganeshr/website-builder.git"
  },
  "bugs": {
    "url": "https://github.com/baalaganeshr/website-builder/issues"
  },
  "homepage": "https://github.com/baalaganeshr/website-builder#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "python": ">=3.11"
  }
}